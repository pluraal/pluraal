{
  "inputs": [
    {
      "name": "customerName",
      "type": "string"
    },
    {
      "name": "orderAmount",
      "type": "number"
    },
    {
      "name": "isPremium",
      "type": "bool"
    }
  ],
  "dataPoints": [
    {
      "name": "discount",
      "expression": {
        "type": "branch",
        "value": {
          "if": {
            "type": "variable",
            "name": "isPremium"
          },
          "then": {
            "type": "literal",
            "value": 0.15
          },
          "else": {
            "type": "literal",
            "value": 0.05
          }
        }
      }
    },
    {
      "name": "discountCategory",
      "expression": {
        "type": "branch",
        "value": {
          "rules": [
            {
              "when": {
                "type": "variable",
                "name": "isPremium"
              },
              "then": {
                "type": "literal",
                "value": "premium"
              }
            }
          ],
          "otherwise": {
            "type": "literal",
            "value": "standard"
          }
        }
      }
    },
    {
      "name": "greeting",
      "expression": {
        "type": "branch",
        "value": {
          "branchOn": {
            "type": "variable",
            "name": "discountCategory"
          },
          "when": {
            "premium": {
              "type": "literal",
              "value": "Thank you for being a Premium customer!"
            },
            "standard": {
              "type": "literal",
              "value": "Thank you for your order!"
            }
          },
          "otherwise": {
            "type": "literal",
            "value": "Hello!"
          }
        }
      }
    }
  ],
  "result": {
    "type": "variable",
    "name": "greeting"
  }
}
